{
    "name": "nasc-touchbar",
    "displayName": "Nasc VSCode Touch Bar",
    "description": "Adds useful tools to your MacBook Touch Bar.",
    "version": "1.7.0",
    "publisher": "felipe",
    "engines": {
        "vscode": "^1.17.0"
    },
    "repository": "https://github.com/NascHQ/nasc-vscode-mac-touchbar",
    "categories": [
        "Other"
    ],
    "keywords": [
        "macbook",
        "touch bar",
        "touchbar",
        "vscode",
        "visual studio code",
        "macbook pro",
        "developers",
        "buttons"
    ],
    "activationEvents": [
        "onCommand:nasc.touchBar.goToDefinition",
        "onCommand:nasc.touchBar.addCursorAbove",
        "onCommand:nasc.touchBar.addCursorBelow"
    ],
    "main": "./extension",
    "capabilities": {
        "definitionProvider": "true"
    },
    "contributes": {
        "commands": [
            {
                "command": "nasc.touchBar.goToDefinition",
                "group": "nasc",
                "when": "editorHasSelection",
                "title": "‚Üí∆í"
            },
            {
                "command": "nasc.touchBar.addCursorAbove",
                "group": "nasc",
                "title": "‚Üëùô∏"
            },
            {
                "command": "nasc.touchBar.addCursorBelow",
                "group": "nasc",
                "title": "‚Üìùô∏"
            },
            {
                "command": "workbench.action.toggleSidebarVisibility",
                "group": "nasc",
                "title": "‚óß"
            },
            {
                "command": "workbench.action.togglePanel",
                "group": "nasc",
                "title": "‚¨ì"
            },
            {
                "command": "workbench.action.showCommands",
                "group": "nasc",
                "title": ">_"
            },
            {
                "command": "editor.action.rename",
                "group": "nasc",
                "title": "‚úé_"
            },
            {
                "command": "editor.action.copyLinesDownAction",
                "group": "nasc",
                "title": "D‚Üì"
            },
            {
                "command": "editor.action.addSelectionToNextFindMatch",
                "group": "nasc",
                "title": "üîé+"
            },
            {
                "command": "editor.action.moveSelectionToNextFindMatch",
                "group": "nasc",
                "title": "üîé"
            },
            {
                "command": "editor.action.toggleRenderWhitespace",
                "group": "nasc",
                "title": "a..b"
            },
            {
                "command": "editor.action.previewDeclaration",
                "group": "nasc",
                "title": "∆í ["
            },
            {
                "command": "editor.action.referenceSearch.trigger",
                "group": "nasc",
                "title": "∆í‚•∫"
            },
            {
                "command": "workbench.action.openGlobalSettings",
                "group": "nasc",
                "title": "‚öô"
            },
            {
                "command": "editor.action.indentLines",
                "group": "nasc",
                "title": "‚Üí‚ò∞"
            },
            {
                "command": "editor.action.outdentLines",
                "group": "nasc",
                "title": "‚Üê‚ò∞"
            },
            {
                "command": "editor.action.jumpToBracket",
                "group": "nasc",
                "title": "‚Üí}"
            },
            {
                "command": "editor.action.blockComment",
                "group": "nasc",
                "title": "/* */"
            },
            {
                "command": "editor.action.commentLine",
                "group": "nasc",
                "title": "//"
            }
        ],
        "menus": {
            "touchBar": [
                {
                    "command": "nasc.touchBar.goToDefinition",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.goToDefinition"
                },
                {
                    "command": "nasc.touchBar.addCursorAbove",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.addCursorAbove"
                },
                {
                    "command": "nasc.touchBar.addCursorBelow",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.addCursorBelow"
                },
                {
                    "command": "workbench.action.toggleSidebarVisibility",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.toggleSidebar"
                },
                {
                    "command": "editor.action.rename",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.rename"
                },
                {
                    "command": "workbench.action.togglePanel",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.togglePanel"
                },
                {
                    "command": "workbench.action.showCommands",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.showCommands"
                },
                {
                    "command": "editor.action.copyLinesDownAction",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.copyLineDown"
                },
                {
                    "command": "editor.action.addSelectionToNextFindMatch",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.selectNext"
                },
                {
                    "command": "editor.action.moveSelectionToNextFindMatch",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.goToNext"
                },
                {
                    "command": "editor.action.toggleRenderWhitespace",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.toggleWhiteSpace"
                },
                {
                    "command": "editor.action.previewDeclaration",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.peekDefinition"
                },
                {
                    "command": "editor.action.referenceSearch.trigger",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.showReferences"
                },
                {
                    "command": "workbench.action.openGlobalSettings",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.settings"
                },
                {
                    "command": "editor.action.indentLines",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.indent"
                },
                {
                    "command": "editor.action.outdentLines",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.outdent"
                },
                {
                    "command": "editor.action.jumpToBracket",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.jumpToBracket"
                },
                {
                    "command": "editor.action.blockComment",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.blockComment"
                },
                {
                    "command": "editor.action.commentLine",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.commentLine"
                }
            ]
        },
        "configuration": [
            {
                "title": "Nasc Touch Bar Settings",
                "type": "object",
                "properties": {
                    "nasc-touchbar.goToDefinition": {
                        "type": "boolean",
                        "default": true,
                        "description": "Go to Definition"
                    },
                    "nasc-touchbar.addCursorBelow": {
                        "type": "boolean",
                        "default": true,
                        "description": "Add cursor below"
                    },
                    "nasc-touchbar.rename": {
                        "type": "boolean",
                        "default": true,
                        "description": "Rename Symbol. It will replace the variable or function name (for example) everywhere"
                    },
                    "nasc-touchbar.togglePanel": {
                        "type": "boolean",
                        "default": true,
                        "description": "Toggle Panel"
                    },
                    "nasc-touchbar.showCommands": {
                        "type": "boolean",
                        "default": true,
                        "description": "Show Commands"
                    },
                    "nasc-touchbar.selectNext": {
                        "type": "boolean",
                        "default": true,
                        "description": "Select the next matching string (if a word is selected, uses this to search)"
                    },
                    "nasc-touchbar.toggleSidebar": {
                        "type": "boolean",
                        "default": false,
                        "description": "Toggle Sidebar"
                    },
                    "nasc-touchbar.addCursorAbove": {
                        "type": "boolean",
                        "default": false,
                        "description": "Add cursor above"
                    },
                    "nasc-touchbar.copyLineDown": {
                        "type": "boolean",
                        "default": false,
                        "description": "Copy the current line below"
                    },
                    "nasc-touchbar.goToNext": {
                        "type": "boolean",
                        "default": false,
                        "description": "Go to the next match (if a word is selected, uses this to search)"
                    },
                    "nasc-touchbar.toggleWhiteSpace": {
                        "type": "boolean",
                        "default": false,
                        "description": "Show or hide white spaces"
                    },
                    "nasc-touchbar.peekDefinition": {
                        "type": "boolean",
                        "default": false,
                        "description": "Peek definition/declaration"
                    },
                    "nasc-touchbar.showReferences": {
                        "type": "boolean",
                        "default": false,
                        "description": "List references who use the current symbol"
                    },
                    "nasc-touchbar.settings": {
                        "type": "boolean",
                        "default": false,
                        "description": "Open user settings"
                    },
                    "nasc-touchbar.indent": {
                        "type": "boolean",
                        "default": false,
                        "description": "Indent text"
                    },
                    "nasc-touchbar.outdent": {
                        "type": "boolean",
                        "default": false,
                        "description": "Outdent text"
                    },
                    "nasc-touchbar.jumpToBracket": {
                        "type": "boolean",
                        "default": false,
                        "description": "Jump to matching bracket"
                    },
                    "nasc-touchbar.blockComment": {
                        "type": "boolean",
                        "default": false,
                        "description": "Toggles block comments ( /* ... */ ) for the current selection"
                    },
                    "nasc-touchbar.commentLine": {
                        "type": "boolean",
                        "default": false,
                        "description": "Toggles line comments ( // ) for the current selection"
                    }
                }
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "icon": "nasc-macbook-pro-vscode-touch-bar.jpg",
    "devDependencies": {
        "typescript": "^2.5.2",
        "vscode": "^1.1.5",
        "mocha": "^3.5.0",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.0",
        "@types/mocha": "^2.2.42"
    }
}
